{
    "name": "netgen/ez-forms-bundle",
    "description": "Integration of Symfony's Form component with Ibexa Platform Public API",
    "type": "ibexa-bundle",
    "license": "GPL-2.0-only",
    "keywords": [
        "ibexa",
        "forms",
        "netgen",
        "ezforms",
        "ez-forms-bundle"
    ],
    "homepage": "https://github.com/netgen/NetgenEzFormsBundle",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "http://netgen.io"
        }
    ],
    "require": {
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/form": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/options-resolver": "^5.4",
        "symfony/property-access": "^5.4",
        "symfony/validator": "^5.4",
        "ibexa/core": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "matthiasnoback/symfony-config-test": "^4.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Netgen\\Bundle\\EzFormsBundle\\": "bundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netgen\\Bundle\\EzFormsBundle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    }
}
